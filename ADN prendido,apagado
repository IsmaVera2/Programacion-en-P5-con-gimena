let img1;
let img2;
let checkbox;

function preload() {
    img2 = loadImage("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRbi2mL2h1Quhq54gDe81ZwR6GvDhdP5G63Zj4M8vmuLM3u3zBoj9SAzPpS3SVBuDYn5WI&usqp=CAU");
    img1 = loadImage(
        "data:image/jpeg;base64,"
    );
}
function setup() {
    createCanvas(400,450);
    background(255);
    checkbox = createCheckbox("ADN Apagado", false);
    checkbox.position(120, 20);
    checkbox1 = createCheckbox("ADN Prendido", false);
    checkbox1.position(120, 40);

}

function draw(){
    background(255);
      if (checkbox.checked()) {
        image(img1, 50, 250);
    }

  
       if (checkbox1.checked()) {
        image(img2, 30, 100);
    }

}
